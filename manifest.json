{
  "name": "LinkedIn to google spreadsheet",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Adds a button on linkedIn profiles for exporting the profile data to a google spreadsheet",
  "homepage_url": "http://example.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "options_page": "src/options/index.html",
  "permissions": ["tabs", "storage", "*://*.linkedin.com/*"],
  "content_scripts": [
    {
      "matches": [
        "*://*.linkedin.com/profile/*"
      ],
      "js": [
        "src/lib/jquery.2.1.1.min.js",
        "src/inject/inject.js"
      ]
    }
  ]
}